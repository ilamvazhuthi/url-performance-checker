
{
  "manifest_version": 3,
  "name": "URL Performance Checker",
  "version": "1.0",
  "description": "Check the average load time of a webpage.",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "storage", "browsingData"],
  "action": {
    "default_popup": "popup.html"
  }
}
